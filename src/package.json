{
  "name": "src",
  "version": "0.1.0",
  "private": true,
  "proxy": {
    "/graphql": {
      "target": "http://localhost:2501"
    },
    "/schema": {
      "target": "http://localhost:2501"
    }
  },
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "bootstrap": "^4.1.1",
    "graphql": "^0.13.2",
    "graphql-relay": "^0.5.5",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-redux": "^5.0.7",
    "react-relay": "^1.6.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "react-select": "^1.2.1",
    "reactstrap": "^6.0.1",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "relay-compiler": "^1.6.0",
    "relay-runtime": "^1.6.0"
  },
  "scripts": {
    "start": "npm-run-all -s setup schema -p relay watch-css start-js",
    "build": "npm-run-all build-css build-js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "relay": "relay-compiler --src ./src/components/ --schema ./src/graphql/schema.graphql --extensions js jsx",
    "schema": "node ./src/graphql/updateSchema.js",
    "setup": "node ./setup"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.6.0"
  }
}
